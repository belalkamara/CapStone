<header class="masthead">
  <div class="overlay"></div>
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
          <div class="site-heading">
            <h1><h1>Ranking Dashboard</h1></h1>
            <span class="subheading">View Runners Best Days</span>
          </div>
        </div>
      </div>
    </div>
  </div>  
</header>

<body>
  <section class="main-content container">
    <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>
            User Name
          </th>
          <th>
            Day
          </th>
          <th>
            Miles
          </th>
        </tr>
      </thead>
      <tbody>
        <%= render @user_log %>
      </tbody>
    </table>
    <div>
      <%= paginate @user_log %>
    </div>
  </section> 
</body>


